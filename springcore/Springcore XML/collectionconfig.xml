<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
 xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
 xmlns:context="http://www.springframework.org/scema/context"
 xsi:schemaLocation="http://www.springframework.org/schema/beans
 http://www.springframework.org/schema/beans/spring-beans.xsd">
 
 <bean id="question1" class="com.springcore.question">
 <property name="questionid" value="5566"></property>
 <property name="question">
 <list>
 <value>what is a spring Framework</value>
 </list>
 </property>
 <property name="answers">
 <set>
 <value>Spring framework was developed by Rod Johnson in 2003</value>
 </set>
 </property>
 <property name="answerssequence">
 <map>
 <entry key="1" value="it is a lightweight framework"/>
 <entry key="2"  value="it is a loosley coupled"/>
 </map>
 </property>
 </bean>
 </beans>